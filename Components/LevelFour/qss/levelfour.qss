/* Basic Palette
border styles: dashed | dot-dash | dot-dot-dash | dotted | double | groove | inset | outset | ridge | solid | none
*/

/* Set the selection colors for all widgets. */
QWidget
{
	color: #c8c8c8;
	background-color: #2e2e2e;
	selection-color: #686868;
	selection-background-color: #222222;
	padding: 0px;
	margin: 0px;
}

QMainWindow
{
	background: #2e2e2e;
	padding: 0px;
	margin: 0px;
}

QMainWindow::separator
{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #555555, stop: 1.0 #3b3b3b);

	border-top: 1px solid #666666;
	border-bottom: 1px solid #262626;
	border-left: 1px solid #666666;
	border-right: 1px solid #262626;
	width: 4px; /* when vertical */
    height: 4px; /* when horizontal */
}

QMainWindow::separator:hover
{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #585858, stop: 1.0 #3b3b3b);

	border-top: 1px solid #666666;
	border-bottom: 1px solid #262626;
	border-left: 1px solid #666666;
	border-right: 1px solid #262626;
}

/* Customize check boxes. */
QCheckBox {
    spacing: 5px;
}

QCheckBox::indicator {
    width: 13px;
    height: 13px;
}

QCheckBox::indicator:unchecked {
    image: url(:/MainWindow/images/checkbox_unchecked.png);
}

QCheckBox::indicator:unchecked:hover {
    image: url(:/MainWindow/images/checkbox_unchecked_hover.png);
}

QCheckBox::indicator:unchecked:pressed {
    image: url(:/MainWindow/images/checkbox_unchecked_pressed.png);
}

QCheckBox::indicator:checked {
    image: url(:/MainWindow/images/checkbox_checked.png);
}

QCheckBox::indicator:checked:hover {
    image: url(:/MainWindow/images/checkbox_checked_hover.png);
}

QCheckBox::indicator:checked:pressed {
    image: url(:/MainWindow/images/checkbox_checked_pressed.png);
}

/* Combobox */
QComboBox 
{
	border-top: 1px solid #1c1c1c;
	border-left: 1px solid #1c1c1c;
	border-bottom: 1px solid #111111;
	border-right: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #555555, stop: 1.0 #3c3c3c);

	color: #c8c8c8;
	border-top-left-radius: 0px; /* same radius as the QComboBox */
	border-bottom-left-radius: 0px;
	padding: 1px 18px 1px 3px;
	min-width: 6em;
}

/* The popup */
QComboBox QAbstractItemView {
	background: #3a3a3a;
	border-top: 1px solid #949494;
	border-left: 1px solid #949494;
	border-bottom: 1px solid #262626;
	border-right: 1px solid #262626;
	selection-background-color: #494949;
	selection-color: #e5e5e5;
	color: #c8c8c8;
}

QComboBox:editable
{
	border-top: 1px solid #1c1c1c;
	border-left: 1px solid #1c1c1c;
	border-bottom: 1px solid #111111;
	border-right: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #555555, stop: 1.0 #3c3c3c);
}

QComboBox:!editable, QComboBox::drop-down:editable
{
	border-top: 1px solid #1c1c1c;
	border-left: 1px solid #1c1c1c;
	border-bottom: 1px solid #111111;
	border-right: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #555555, stop: 1.0 #3c3c3c);
}

/* QComboBox gets the "on" state when the popup is open */
QComboBox:!editable:on, QComboBox::drop-down:editable:on
{
	border-top: 1px solid #1c1c1c;
	border-left: 1px solid #1c1c1c;
	border-bottom: 1px solid #111111;
	border-right: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #555555, stop: 1.0 #3c3c3c);
}

QComboBox:on
{ 
	/* shift the text when the popup opens */
	padding-top: 3px;
	padding-left: 4px;
	border-top: 1px solid #1c1c1c;
	border-left: 1px solid #1c1c1c;
	border-bottom: 1px solid #111111;
	border-right: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #555555, stop: 1.0 #3c3c3c);
}

/* Drop down button */
QComboBox::drop-down
{
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 15px;
	background: #494949;
	border-top: 0px solid #818181;
	border-left: 0px solid #818181;
	border-bottom: 0px solid #2b2b2b;
	border-right: 0px solid #2b2b2b;

}

QComboBox::down-arrow
{
	border: 1px solid #303030;
	background: #848484;
	width: 3px;
    height: 3px;
}

QComboBox::down-arrow:on
{
	/* shift the arrow when popup is open */
	top: 1px;
	left: 1px;
	border: 1px solid #303030;
	background: #848484;

}

QDockWidget
{
    border-top: 1px solid #606060;
	border-left: 1px solid #606060;
	border-bottom: 1px solid #303030;
	border-right: 1px solid #303030;
	border-radius: 5px;
    padding: 2px;
    margin: 2px;
}

QDockWidget::title
{
    background: #2e2e2e;
}

/* Header for ... */
QHeaderView::section
{
	color: #c8c8c8;
	padding-left: 4px;
	border-top: 1px solid #1c1c1c;
	border-left: 1px solid #1c1c1c;
	border-bottom: 1px solid #111111;
	border-right: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #555555, stop: 1.0 #3c3c3c);
}

/* Text input box */
QLineEdit
{
	color: #ffffff;
    background: #131313;
    border-bottom: 1px solid #242424;
	border-right: 1px solid #242424;
	border-top: 1px solid #2e2e2e;
	border-left: 1px solid #2e2e2e;
	padding: 0 8px;
	selection-background-color: #232323;
	margin-left: 5px;
	margin-right: 5px;
}

QLabel
{
	margin-left: 5px;
	margin-right: 5px;
	background: none;
}

/* Drop down style */
QMenu
{
	background: #3a3a3a;
	border-top: 1px solid #949494;
	border-left: 1px solid #949494;
	border-bottom: 1px solid #161616;
	border-right: 1px solid #161616;
	color: #c8c8c8;
}

QMenu::item
{
	/* sets background of menu item. set this to something non-transparent
	if you want menu color and menu item color to be different */
	background-color: transparent;
}

QMenu::item:selected
{
	/* when user selects item using mouse or keyboard */
	background: #494949;
	color: #e5e5e5;
}

QMenuBar
{
    border-top: 1px solid #424242;
	border-left: 1px solid #424242;
	border-bottom: 1px solid #262626;
	border-right: 1px solid #262626;
		background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #3a3a3a, stop: 1.0 #323232);

}

QMenuBar::item
{
	spacing: 3px; /* spacing between menu bar items */
	padding: 1px 4px;
	background: transparent;
	color: #c8c8c8;
}

/* when selected using mouse or keyboard */

QMenuBar::item:selected
{
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #b98620, stop: 1.0 #936a19);
	color: #e5e5e5;
	border-radius: 0px;
}

QMenuBar::item:pressed
{
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #b98620, stop: 1.0 #936a19);
    color: #e5e5e5;
	border-radius: 0px;
}

QPushButton
{
	min-width: 80px;
	border-top: 1px solid #1c1c1c;
	border-left: 1px solid #1c1c1c;
	border-bottom: 1px solid #111111;
	border-right: 1px solid #111111;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #555555, stop: 1.0 #3a3a3a);
	color: #c8c8c8;
	border-radius: 5px;
}

/* all types of push button */
QPushButton:hover
{
	color: #cccccc;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #b98620, stop: 1.0 #936a19);

	border-radius: 5px;
}

QPushButton:pressed
{
	color: #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #b98620, stop: 1.0 #936a19);

	border-radius: 5px;
}

