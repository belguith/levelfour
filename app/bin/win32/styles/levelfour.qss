/* Basic Palette
border styles: dashed | dot-dash | dot-dot-dash | dotted | double | groove | inset | outset | ridge | solid | none
*/

/* Set the selection colors for all widgets. */
QWidget
{
	color: #c8c8c8;
	border: 0px solid #737373;
	background: #686868;
	selection-color: #686868;
	selection-background-color: #6b6b6b;
	padding: 0px;
	margin: 0px;
}

QMainWindow
{
	background: #686868;
	padding: 0px;
	margin: 0px;
	border: 0px solid #1a1a1a;
}

QMainWindow::separator:horizontal
{
    background: #686868;
    max-width: 1px;
	width: 1px;
	border-top: 1px solid #393939;
	border-bottom: 1px solid #959595;
}

QMainWindow::separator:vertical
{
    background: #686868;
    max-height: 1px;
	height: 1px;
	border-left: 1px solid #393939;
	border-right: 1px solid #959595;
}

QMainWindow::separator:hover
{
    background: #686868;
}

/* Customize check boxes. */
QCheckBox 
{
    spacing: 5px;
   	border: 1px solid #222222;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #515151, stop:0.4 #444444, stop:1 #373737);
	max-width: 13px;
    max-height: 13px;
}

QCheckBox::indicator
{
    width: 13px;
    height: 13px;
}

QCheckBox::indicator:unchecked
{
    image: url(:/checkbox_unchecked);
}

QCheckBox::indicator:unchecked:hover
{
    image: url(:/checkbox_unchecked_hover);
}

QCheckBox::indicator:unchecked:pressed
{
    image: url(:/checkbox_unchecked_pressed);
}

QCheckBox::indicator:checked
{
    image: url(:/checkbox_checked);
}

QCheckBox::indicator:checked:hover
{
    image: url(:/checkbox_checked_hover);
}

QCheckBox::indicator:checked:pressed
{
    image: url(:/checkbox_checked_pressed);
}

/* Combobox */
QComboBox 
{
	border: 1px solid #222222;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #515151, stop:0.4 #444444, stop:1 #373737);
	color: #c8c8c8;
	border-top-left-radius: 6px; /* same radius as the QComboBox */
	border-bottom-left-radius: 6px;
	border-top-right-radius: 6px;
	border-bottom-right-radius: 6px;
	padding: 1px 18px 1px 3px;
	min-width: 6em;
	max-height: 15px;
}

/* The popup */
QComboBox QAbstractItemView {
	border: 1px solid #111111;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #212121, stop:1 #1c1c1c);
	selection-background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #79a3e5, stop:1 #567fc1);;
	selection-color: #e5e5e5;
	color: #c8c8c8;
}

QComboBox:editable
{
	border: 1px solid #222222;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #515151, stop:0.4 #444444, stop:1 #373737);
	
}

QComboBox:!editable, QComboBox::drop-down:editable
{
	border: 1px solid #222222;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #515151, stop:0.4 #444444, stop:1 #373737);
	
}

/* QComboBox gets the "on" state when the popup is open */
QComboBox:!editable:on, QComboBox::drop-down:editable:on
{
	border: 1px solid #222222;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #515151, stop:0.4 #444444, stop:1 #373737);
	
}

QComboBox:on
{ 
	/* shift the text when the popup opens */
	padding-top: 3px;
	padding-left: 4px;
	border: 1px solid #222222;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #515151, stop:0.4 #444444, stop:1 #373737);
}

/* Drop down button */
QComboBox::drop-down
{
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 15px;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #515151, stop:0.4 #444444, stop:1 #373737);
	border: 0px solid #818181;
	border-top-right-radius: 6px; /* same radius as the QComboBox */
	border-bottom-right-radius: 6px;
}

QComboBox::down-arrow
{
     image: url(:/arrow_down);
     width: 11px;
     height: 6px;
}

QComboBox::up-arrow
{
     image: url(:/arrow_up);
     width: 11px;
     height: 6px;
}

QDockWidget
{
    border-top: 1px solid #1a1a1a;
	border-bottom: 1px solid #1a1a1a;
	border-radius: 0px;
}

QDockWidget::title
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #4c4c4c, stop:1 #434343);
}

QFrame
{
     border: 0px solid #737373;
     border-radius: 0px;
     padding: 0px;
     background-color: transparent;
}
 
QGroupBox
 {
     border: 1px solid #383838;
     background: #727272;
     border-radius: 5px;
     margin-top: 13px;
 }

QGroupBox::title
{
     subcontrol-origin: margin; 
     subcontrol-position: top left;
     padding: 0px 10px;
     color: #111111;
}

/* Header for ... */
QHeaderView::section
{
	color: #c8c8c8;
	background: #686868;
	padding-left: 4px;
	border-top: 1px solid #393939;
	border-bottom: 1px solid #959595;
	min-height: 15px;
}

/* Text input box */
QLineEdit
{
	color: #111111;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #9f9f9f, stop:1 #b2b2b2);
    border: 1px solid #323232;
	padding: 0 8px;
	selection-background-color: #222222;
	selection-color: #f5f5f5;
	margin-left: 5px;
	margin-right: 5px;
	border-radius: 5px;
	max-height: 14px;
	height: 14px;
}

QLabel
{
	margin-left: 5px;
	margin-right: 5px;
	background: none;
	border: 0px;
}

/* Drop down style */
QMenu
{
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #212121, stop:1 #1c1c1c);
	border: 1px solid #111111;
	color: #eaeaea;
}

QMenu::separator 
{
     image: none;
     border-top: 1px solid #262626;
}

QMenu::item
{
	/* sets background of menu item. set this to something non-transparent
	if you want menu color and menu item color to be different */
	background-color: transparent;
}

QMenu::item:selected
{
	/* when user selects item using mouse or keyboard */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #79a3e5, stop:1 #567fc1);
	color: #f5f5f5;
}

QMenuBar
{
    border-top: 1px solid #8b8b8b;
	border-left: 0px solid #606060;
	border-bottom: 1px solid #474747;
	border-right: 0px solid #303030;
	background: #686868;
	min-height: 15px;
}

QMenuBar::item
{
	spacing: 3px; /* spacing between menu bar items */
	padding: 1px 4px;
	background: transparent;
	color: #111111;
}

/* when selected using mouse or keyboard */

QMenuBar::item:selected
{
	background: #5680c2;
	color: #000000;
	border-radius: 5px;
	border: 0px;
}

QMenuBar::item:pressed
{
	background: #5680c2;
	color: #000000;
	border-radius: 5px;
	border: 0px;
}

QPushButton
{
    color: #c8c8c8;
    border: 1px solid #222222;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #515151, stop:0.4 #444444, stop:1 #373737);
	border-radius: 5px;
	padding-left: 5px;
	padding-right: 5px;
}

/* all types of push button */
QPushButton:hover
{
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #79a3e5, stop:1 #567fc1);
	color: #111111;
}

QPushButton:pressed
{
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #878787, stop:0.1 #6f6f6f, stop:0.9 #545454, stop:1 #4b4b4b);
	color: #e5e5e5;
}

/* Customize radio buttons. */
QRadioButton 
{
    spacing: 5px;
}

QRadioButton::indicator 
{
    width: 13px;
    height: 13px;
}

QRadioButton::indicator::unchecked
{
    image: url(:/radiobutton_unchecked);
}

QRadioButton::indicator:unchecked:hover 
{
    image: url(:/radiobutton_unchecked_hover);
}

QRadioButton::indicator:unchecked:pressed 
{
    image: url(:/radiobutton_unchecked_pressed);
}

QRadioButton::indicator::checked 
{
    image: url(:/radiobutton_checked);
}

QRadioButton::indicator:checked:hover 
{
    image: url(:/radiobutton_checked_hover);
}

QRadioButton::indicator:checked:pressed 
{
    image: url(:/radiobutton_checked_pressed);
}

/* Customize arrows. 
*::down-arrow, *::menu-indicator {
    image: url(:/arrow_down);
    width: 9px;
    height: 9px;
}

*::down-arrow:disabled, *::down-arrow:off {
   image: url(:/down_arrow_disabled.png);
}

*::up-arrow {
    image: url(:/arrow_up);
    width: 9px;
    height: 9px;
}

*::up-arrow:disabled, *::up-arrow:off {
   image: url(:/up_arrow_disabled.png);
}

*/

QScrollBar QAbstractScrollArea
{
	background: #727272;
}

QScrollBar:horizontal
{
	max-height: 12px;
	min-height: 12px;
	margin: 0px 10px 0px 10px;
    border: 1px solid #3a3a3a;
    border-radius: 5px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #575757, stop:1 #5c5c5c);
}

QScrollBar::handle:horizontal
{
	border: 1px solid #3a3a3a;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #949494, stop:1 #7e7e7e);
	border-radius: 5px;
	min-height: 8px;
	max-height: 8px;
	height: 8px;
}

QScrollBar::add-line:horizontal
{
	border: 0px solid #262626;
    background-color: transparent;
    width: 0px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal
{
	border: 0px solid #262626;
    background-color: transparent;
    width: 0px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::left-arrow:horizontal
{
	border: 0px solid #262626;
    background-color: transparent;
    width: 0px;
}
  
QScrollBar::right-arrow:horizontal
{
	border: 0px solid #262626;
    background-color: transparent;
    width: 0px;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
    background-color: transparent;
}

QScrollBar:vertical 
{
    border: 1px solid #3a3a3a;
    border-radius: 5px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #575757, stop:1 #5c5c5c);
    max-width: 12px;
	min-width: 12px;
	margin: 10px 0px 10px 0px;
}

QScrollBar::handle:vertical
{
	border: 1px solid #3a3a3a;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #949494, stop:1 #7e7e7e);
	border-radius: 5px;
	min-width: 8px;
	max-width: 8px;
	min-height: 20px;
	max-height: 20px;
	width: 8px;

}

QScrollBar::add-line:vertical
{
    	subcontrol-position: top;
    	subcontrol-origin: margin;
		border: 0px solid #262626;
		background-color: transparent;
		height: 0px;
}

QScrollBar::sub-line:vertical
{
    subcontrol-position: bottom;
    subcontrol-origin: margin;
	border: 0px solid #262626;
	background-color: transparent;
	height: 0px;
}

QScrollBar::up-arrow:vertical
{
	border: 0px solid #262626;
	background-color: transparent;
    width: 0px;
    height: 0px;
}
  
QScrollBar::down-arrow:vertical
{
	border: 0px solid #262626;
	background-color: transparent;
    width: 0px;
    height: 0px;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
	background-color: transparent;
}

QSlider::groove:vertical
{
	background-color: transperant;
	position: absolute; 
	left: 4px; right: 4px;
}

QSlider::handle:vertical
{
     height: 10px;
     background: #ff7603;
     border: 1px solid #62340e;
     margin: 0 -4px; /* expand outside the groove */
}

QSlider::add-page:vertical
{
     background: #222222;
}

QSlider::sub-page:vertical
{
     background: #222222;
}

QSpinBox {
    padding-right: 15px; /* make room for the arrows */
	background: #a6a6a6;
    border: 1px solid #0d0d0d;
    color: #111111;
   	border-top-left-radius: 6px; /* same radius as the QComboBox */
	border-bottom-left-radius: 6px;

    
}
 
QSpinBox::up-button
{
    subcontrol-origin: border;
    subcontrol-position: top right; /* position at the top right corner */
    width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
	border: 1px solid #222222;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #a6a6a6, stop:1 #8a8a8a);
}

QSpinBox::up-button:hover
{
	background: #464646;
}

QSpinBox::up-button:pressed
{
	background: #464646;
}

QSpinBox::up-arrow
{
     image: url(:/arrow_up);
     width: 11px;
     height: 6px;
}

QSpinBox::up-arrow:disabled, QSpinBox::up-arrow:off
{ /* off state when value is max */
    image: url(:/arrow_up);
}

QSpinBox::down-button
{
    subcontrol-origin: border;
    subcontrol-position: bottom right; /* position at bottom right corner */
    width: 16px;
	border-top: 0px solid #222222;
	border-left: 1px solid #222222;
	border-bottom: 1px solid #222222;
	border-right: 1px solid #222222;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #a6a6a6, stop:1 #8a8a8a);
    border-top-width: 0;
}

QSpinBox::down-button:hover
{
	background: #464646;
}

QSpinBox::down-button:pressed
{
	background: #464646;
}

QSpinBox::down-arrow
{
     image: url(:/arrow_down);
     width: 11px;
     height: 6px;
}

QSpinBox::down-arrow:disabled,
QSpinBox::down-arrow:off 
{ /* off state when value in min */
    image: url(:/arrow_down);
}

QSplitter::handle 
{
    background: #262626;
}

QSplitter::handle:horizontal
{
 	background: #262626;
 	max-width: 1px;
	width: 1px;
	border-left: 1px solid #393939;
	border-right: 1px solid #959595;
}

QSplitter::handle:vertical
{
   	background: #262626;
   	max-height: 1px;
	height: 1px;
	border-top: 1px solid #393939;
	border-bottom: 1px solid #959595;
}

QStatusBar
{
   	border-bottom: 1px solid #606060;
	border-top: 1px solid #262626;
	background: #686868;
}

/* Table View */
QTableView
{
	selection-background-color: #a6a6a6;
	selection-color: #d2b450;
	padding-left: 5px;
	padding-right: 5px;
	color: #000000;
}

QTreeView
{
	margin-top: 1px;
	border: 0px solid #434343;
	border-radius: 0px;
	padding: 0px;
	background: #727272;
	paint-alternating-row-colors-for-empty-area: 1;
	show-decoration-selected: 1;
	alternate-background-color: #787878;
}

QTreeView::item 
{
	color: #111111;
	padding-left: 10px;
}

QTreeView::item:hover
{
	color: #e5e5e5;
}

QTreeView::item:selected
{
	color: #f5f5f5;
	background: #686868;
}

QTreeView::item:selected:active
{
	color: #f5f5f5;
	background: #686868;
}

QTreeView::item:selected:!active
{
	color: #f5f5f5;
	background: #686868;
}

QTreeView::branch:has-siblings:!adjoins-item 
{
     border-image: url(:/vline) 0;
}

QTreeView::branch:has-siblings:adjoins-item 
{
     border-image: url(:/branch-more) 0;
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item
{
     border-image: url(:/branch-end) 0;
}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings
{
         image: url(:/branch-closed);
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings
{
         border-image: none;
         image: url(:/branch-open);
}

/* The tab widget frame */
QTabWidget::pane
{
    border: 1px solid #737373;
	margin: 0px;
	padding: 0px;
	background: #727272;
}

QTabWidget::tab-bar
{
	background: #727272;
	border: 1px solid #737373;
}

/* Style the tab using the tab sub-control. */
QTabBar::tab
{
	border: 1px solid #222222;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #535353, stop:1 #363636);
	color: #afafaf;
	padding: 3px;
	border-radius: 0px;
	margin-top: 1px;
	margin-bottom: 3px;
	margin-left: 0px;
}

QTabBar::tab:hover
{
	color: #c8c8c8;
}

QTabBar::tab:selected
{
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #4670b2, stop:1 #638dcf);
	color: #e5e5e5;
}

/* Style for main toolbar */
QToolBar
{
    border: 0px solid #8b8b8b;
	background-color: transparent;
	spacing: 2px; /* spacing between items in the tool bar */
	margin-left: 3px;
	color: #e5e5e5;
	max-height: 22px;
}

QToolBar::handle
{
     image: none;
     background-color: transparent;
}
 
QToolBar::separator 
{
     width: 5px;
     border: 0px;
     background-color: transparent;
}

/* All types of tool button */
QToolButton
{
	border: 1px solid #323232;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #999999, stop:1 #b0b0b0);
	color: #111111;
	border-radius: 5px;
	max-height: 18px;
	min-width: 18px;
}

QToolButton[popupMode="1"]
{
	/* only for MenuButtonPopup */
	padding-right: 20px; /* make way for the popup button */
	/* max-width: 32px; */
}

QToolButton::menu-button
{
     /* 16px width + 4px for border = 20px allocated above */
	border-top: 1px solid #323232;
	border-left: 1px solid #222222;
	border-bottom: 1px solid #323232;
	border-right: 1px solid #323232;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #a6a6a6, stop:1 #8a8a8a);
	color: #111111;
}

QToolButton:hover
{
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #79a3e5, stop:1 #567fc1);
	color: #c8c8c8;
}

QToolButton:pressed
{
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #383838, stop:0.1 #424242, stop:0.9 #4c4c4c, stop:1 #404040);
	color: #c8c8c8;
}

QToolTip
{
	border: 1px solid #ffffff;
	background: #333333;
	padding: 3px;
	border-radius: 5px;
	opacity: 150;
	color: #CCCCCC;
}
