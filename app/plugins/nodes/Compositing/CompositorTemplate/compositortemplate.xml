<!DOCTYPE nodetype SYSTEM "nodetype.dtd">
<nodetype name="CompositorTemplate" category="Rendering" color="7, 45, 87" plugin="compositortemplate.dll">
  <parameters>
    <parameter name="Resource Group Name" type="String" defaultValue="CompositorTemplateNode"/>
    <parameter name="Reload" type="Command"/>
    <parameter name="Input Map Scene" type="Image" pin="in" selfEvaluating="true"/>
    <parameter name="Input Map Normal" type="Image" pin="in" selfEvaluating="true"/>
    <parameter name="Input Map Depth" type="Image" pin="in" selfEvaluating="true"/>
    <parameter name="Param 0" type="Float" defaultValue="1.0" minValue="-1.0" maxValue="1.0" stepSize="0.01"/>
    <parameter name="Param 1" type="Float" defaultValue="1.0" minValue="-1.0" maxValue="1.0" stepSize="0.01"/>
    <parameter name="Param 2" type="Float" defaultValue="1.0" minValue="-1.0" maxValue="1.0" stepSize="0.01"/>
    <parameter name="Param 3" type="Float" defaultValue="1.0" minValue="-1.0" maxValue="1.0" stepSize="0.01"/>
    <parameter name="Param 4" type="Float" defaultValue="1.0" minValue="-1.0" maxValue="1.0" stepSize="0.01"/>
  </parameters>
</nodetype>


