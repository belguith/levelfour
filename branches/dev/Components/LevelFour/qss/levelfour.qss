QScrollBar QAbstractScrollArea
{
	background: #404040;
}

QScrollBar:horizontal
{
	background: #404040;
	max-height: 12px;
	min-height: 12px;
	margin: 0px 22px 0px 22px;
    border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;

}

QScrollBar::handle:horizontal
{
	border-top: 1px solid #838383;
	border-left: 1px solid #838383;
	border-bottom: 1px solid #636363;
	border-right: 1px solid #636363;
	background: #606060;
	border-radius: 5px;
	min-width: 20px;
	max-width: 20px;
	min-height: 8px;
	max-height: 8px;
	height: 8px;
}

QScrollBar::add-line:horizontal
{
	background: #4d4d4d;
    border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;

	width: 20px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal
{
	background: #4d4d4d;	
	border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;

	width: 20px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::left-arrow:horizontal
{
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;
	border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
    background: #585858;
    width: 6px;
    height: 6px;

  }
  
QScrollBar::right-arrow:horizontal
{
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;
	border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
    background: #585858;
    width: 6px;
    height: 6px;

}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
     background: #4d4d4d;
}

QScrollBar:vertical 
{
	background: #404040;
	max-width: 12px;
	min-width: 12px;
	margin: 22px 0px 22px 0px;
	border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;

}

QScrollBar::handle:vertical
{
	border-top: 1px solid #838383;
	border-left: 1px solid #838383;
	border-bottom: 1px solid #636363;
	border-right: 1px solid #636363;
	background: #606060;
	border-radius: 5px;
	min-width: 8px;
	max-width: 8px;
	min-height: 20px;
	max-height: 20px;
	width: 8px;

}

QScrollBar::add-line:vertical
{
    background: #4d4d4d;
    height: 20px;
    subcontrol-position: top;
    subcontrol-origin: margin;
    border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;

}

QScrollBar::sub-line:vertical
{
    background: #4d4d4d;
    height: 20px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
    border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;

}

QScrollBar::up-arrow:vertical
{
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;
	border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
    background: #585858;
    width: 6px;
    height: 6px;

  }
  
QScrollBar::down-arrow:vertical
{
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;
	border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
    background: #585858;
    width: 6px;
    height: 6px;

}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
      background: #4d4d4d;
  }

QSlider::groove:vertical
{
	background: #333333;
	position: absolute; 
	left: 4px; right: 4px;
}

QSlider::handle:vertical
{
     height: 10px;
     background: #ff7603;
     border: 1px solid #62340e;
     margin: 0 -4px; /* expand outside the groove */
}

QSlider::add-page:vertical
{
     background: #686868;
}

QSlider::sub-page:vertical
{
     background: #686868;
}

QSpinBox {
    padding-right: 15px; /* make room for the arrows */
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;
	border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	background: #585858;
}
 
QSpinBox::up-button {
     subcontrol-origin: border;
     subcontrol-position: top right; /* position at the top right corner */
     width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
     /*
     border-image: url(:/images/spinup.png) 1;
     */
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;
	border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	background: #565656;
 }

 QSpinBox::up-button:hover
 {
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
 }

 QSpinBox::up-button:pressed
 {
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
}

 QSpinBox::up-arrow {
     image: url(:/MainWindow/images/up_arrow.png);
     width: 11px;
     height: 6px;
 }

 QSpinBox::up-arrow:disabled, QSpinBox::up-arrow:off
 { /* off state when value is max */
    image: url(:/MainWindow/images/up_arrow.png);
 }

 QSpinBox::down-button {
     subcontrol-origin: border;
     subcontrol-position: bottom right; /* position at bottom right corner */
     width: 16px;
	border-top: 0px solid #303030;
	border-left: 1px solid #303030;
	border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	background: #585858;
    border-top-width: 0;
 }

 QSpinBox::down-button:hover
 {
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
 }

 QSpinBox::down-button:pressed
 {
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
}

 QSpinBox::down-arrow {
     image: url(:/MainWindow/images/down_arrow.png);
     width: 11px;
     height: 6px;
 }

 QSpinBox::down-arrow:disabled,
 QSpinBox::down-arrow:off { /* off state when value in min */
    image: url(:/MainWindow/images/down_arrow.png);
 }

QSplitter::handle 
{
    background: #333333;
	border-top: 0px solid #666666;
	border-bottom: 0px solid #262626;
	border-left: 0px solid #666666;
	border-right: 0px solid #262626;
}

QSplitter::handle:horizontal
{
     width: 4px;
}

QSplitter::handle:vertical
{
     height: 4px;
}

QStatusBar
{
    border-top: 1px solid #606060;
	border-left: 1px solid #606060;
	border-bottom: 1px solid #303030;
	border-right: 1px solid #303030;
	background: #404040;
}


/* Table View */
QTableView
{
	selection-background-color: #a6a6a6;
	selection-color: #d2b450;
	padding-left: 5px;
	padding-right: 5px;
	color: #000000;
}

/* TreeView */
QTreeView
{
	margin: 3px;
    border-bottom: 1px solid #606060;
	border-right: 1px solid #606060;
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;
	border-radius: 0px;
	padding: 0px;
	background: #404040;
	color: #c8c8c8;
	paint-alternating-row-colors-for-empty-area: 0;
	show-decoration-selected: 1;
	alternate-background-color: #787878;

}

QTreeView::item 
{
	background: #4e4e4e;
	color: #c8c8c8;
    border-bottom: 1px solid #606060;
	border-right: 1px solid #606060;
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;
	border-radius: 0px;
	padding-left: 15px;
}

QTreeView::item:hover
{
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
	color: #e5e5e5;
}
QTreeView::item:selected
{
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
	color: #e5e5e5;
}

QTreeView::item:selected:active
{
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
	color: #e5e5e5;
}

QTreeView::item:selected:!active
{
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
	color: #e5e5e5;
}

QTreeView::branch:has-siblings:!adjoins-item {
     /* border-image: url(:/MainWindow/images/vline.png) 0; */
     border-image: none;
 }

 QTreeView::branch:has-siblings:adjoins-item {
     /* border-image: url(:/MainWindow/images/branch-more.png) 0; */
     border-image: none;
 }

 QTreeView::branch:!has-children:!has-siblings:adjoins-item {
     /* border-image: url(:/MainWindow/images/branch-end.png) 0; */
     border-image: none;
 }

 QTreeView::branch:has-children:!has-siblings:closed,
 QTreeView::branch:closed:has-children:has-siblings {
         /* border-image: none; */
         image: url(:/MainWindow/images/branch-closed.png);
 }

 QTreeView::branch:open:has-children:!has-siblings,
 QTreeView::branch:open:has-children:has-siblings  {
         border-image: none;
         image: url(:/MainWindow/images/branch-open.png);
 }

/* The tab widget frame */
QTabWidget::pane
{
    border-top: 1px solid #606060;
	border-left: 1px solid #606060;
	border-bottom: 1px solid #303030;
	border-right: 1px solid #303030;
	border-radius: 0px;
	margin: 2px;
	padding: 2px;
	background: #404040;

}

QTabWidget::tab-bar
{
	background: #404040;
}

/* Style the tab using the tab sub-control. */
QTabBar::tab
{
	border-top: 1px solid #818181;
	border-left: 1px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #565656;
	color: #a8a8a8;
	padding-left: 4px;
	padding-right: 4px;
	padding-top: 2px;
	padding-bottom: 2px;
	border-radius: 5px;
	margin-top: 3px;
	margin-bottom: 3px;
	margin-left: 6px;
}

QTabBar::tab:hover
{
	border-top: 1px solid #818181;
	border-left: 1px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #505050;
	color: #c8c8c8;
}

QTabBar::tab:selected
{
	border-top: 1px solid #818181;
	border-left: 1px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #505050;
	color: #c8c8c8;

}

/* Style for main toolbar */
QToolBar
{
    border-top: 1px solid #606060;
	border-left: 0px solid #606060;
	border-bottom: 1px solid #303030;
	border-right: 0px solid #303030;
	background: #404040;
	spacing: 3px; /* spacing between items in the tool bar */
	padding-top: 3px;
	padding-bottom: 3px;
	padding-left: 5px;
	margin-bottom: 1px;
	min-height: 25px;
	color: #e5e5e5;
}

QToolBar::handle
{
     image: none;
}
 
QToolBar::separator 
{
     width: 5px;
     border: 0px;
}

/* All types of tool button */
QToolButton
{
	border-top: 1px solid #818181;
	border-left: 1px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #565656;
	color: #c8c8c8;
	border-radius: 5px;
	height: 18px;

}

QToolButton[popupMode="1"]
{
	/* only for MenuButtonPopup */
	padding-right: 20px; /* make way for the popup button */
}

QToolButton::menu-button
{
     /* 16px width + 4px for border = 20px allocated above */
	width: 16px;
	border-top: 1px solid #818181;
	border-left: 0px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #565656;
	color: #c8c8c8;
}

QToolButton:hover
{
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
	color: #c8c8c8;

}

QToolButton:pressed
{
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #151515;
	border-right: 1px solid #151515;
	background: #464646;
	color: #c8c8c8;
}

QToolTip
{
	border: 1px solid #ffffff;
	background: #333333;
	padding: 3px;
	border-radius: 5px;
	opacity: 150;
	color: #CCCCCC;
}
