/* Basic Palette
border styles: dashed | dot-dash | dot-dot-dash | dotted | double | groove | inset | outset | ridge | solid | none
*/

/* Set the selection colors for all widgets. */
QWidget
{
	color: #c8c8c8;
	background-color: #404040;
	selection-color: #686868;
	selection-background-color: #222222;
	padding: 0px;
	margin: 0px;
}

QMainWindow
{
	background: #404040;
	padding: 0px;
	margin: 0px;
}

QMainWindow::separator
{
    background: #404040;
	border-top: 1px solid #787878;
	border-bottom: 1px solid #303030;
	border-left: 1px solid #787878;
	border-right: 1px solid #303030;
	width: 2px; /* when vertical */
    height: 2px; /* when horizontal */
}

QMainWindow::separator:hover
{
    background: #505050;
	border-top: 1px solid #787878;
	border-bottom: 1px solid #303030;
	border-left: 1px solid #787878;
	border-right: 1px solid #303030;
}

/* Customize check boxes. */
QCheckBox {
    spacing: 5px;
}

QCheckBox::indicator {
    width: 13px;
    height: 13px;
}

QCheckBox::indicator:unchecked {
    image: url(:/MainWindow/images/checkbox_unchecked.png);
}

QCheckBox::indicator:unchecked:hover {
    image: url(:/MainWindow/images/checkbox_unchecked_hover.png);
}

QCheckBox::indicator:unchecked:pressed {
    image: url(:/MainWindow/images/checkbox_unchecked_pressed.png);
}

QCheckBox::indicator:checked {
    image: url(:/MainWindow/images/checkbox_checked.png);
}

QCheckBox::indicator:checked:hover {
    image: url(:/MainWindow/images/checkbox_checked_hover.png);
}

QCheckBox::indicator:checked:pressed {
    image: url(:/MainWindow/images/checkbox_checked_pressed.png);
}

/* Combobox */
QComboBox 
{
	border-top: 1px solid #818181;
	border-left: 1px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #565656;
	color: #c8c8c8;
	border-top-left-radius: 3px; /* same radius as the QComboBox */
	border-bottom-left-radius: 3px;
	padding: 1px 18px 1px 3px;
	min-width: 6em;
	height: 18px;
}

/* The popup */
QComboBox QAbstractItemView {
	border: 1px solid #303030;
	background: #3c3c3c;
	selection-background-color: #464646;
	selection-color: #e5e5e5;
	color: #c8c8c8;
}

QComboBox:editable
{
	border-top: 1px solid #818181;
	border-left: 1px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #565656;
}

QComboBox:!editable, QComboBox::drop-down:editable
{
	border-top: 1px solid #818181;
	border-left: 1px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #565656;
}

/* QComboBox gets the "on" state when the popup is open */
QComboBox:!editable:on, QComboBox::drop-down:editable:on
{
	border-top: 1px solid #818181;
	border-left: 1px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #565656;
}

QComboBox:on
{ 
	/* shift the text when the popup opens */
	padding-top: 3px;
	padding-left: 4px;
	border-top: 1px solid #818181;
	border-left: 1px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #565656;
}

/* Drop down button */
QComboBox::drop-down
{
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 15px;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
		stop: 0 #a6a6a6, stop: 1.0 #8a8a8a);
	border-top: 0px solid #818181;
	border-left: 0px solid #818181;
	border-bottom: 0px solid #2b2b2b;
	border-right: 0px solid #2b2b2b;
	background: #565656;

}

QComboBox::down-arrow
{
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;
	border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	background: #585858;
	width: 8px;
    height: 8px;
}

QComboBox::down-arrow:on
{
	/* shift the arrow when popup is open */
	top: 1px;
	left: 1px;
	border: 1px solid #303030;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
		stop: 0 #a6a6a6, stop: 1.0 #8a8a8a);

}

QDockWidget
{
    border-top: 1px solid #606060;
	border-left: 1px solid #606060;
	border-bottom: 1px solid #303030;
	border-right: 1px solid #303030;
	border-radius: 5px;
    padding: 2px;
    margin: 2px;
}

QDockWidget::title
{
    background: #404040;
}

/* Header for ... */
QHeaderView::section
{
	color: #c8c8c8;
	background: #3c3c3c;
	padding-left: 4px;
    border-top: 1px solid #606060;
	border-left: 1px solid #606060;
	border-bottom: 1px solid #303030;
	border-right: 1px solid #303030;
	border-radius-top: 5px;
}

/* Text input box */
QLineEdit
{
	color: #ffffff;
    background: #585858;
    border-bottom: 1px solid #686868;
	border-right: 1px solid #686868;
	border-top: 1px solid #303030;
	border-left: 1px solid #303030;
	padding: 0 8px;
	selection-background-color: #606060;
	margin-left: 5px;
	margin-right: 5px;
}

QLabel
{
	margin-left: 5px;
	margin-right: 5px;
	background: none;
}

/* Drop down style */
QMenu
{
	background: #383838;
	border: 1px solid #2e2e2e;
	color: #c8c8c8;
}

QMenu::item
{
	/* sets background of menu item. set this to something non-transparent
	if you want menu color and menu item color to be different */
	background-color: transparent;
}

QMenu::item:selected
{
	/* when user selects item using mouse or keyboard */
	background: #464646;
	color: #e5e5e5;
}

QMenuBar
{
    border-top: 1px solid #606060;
	border-left: 1px solid #606060;
	border-bottom: 1px solid #303030;
	border-right: 1px solid #303030;
	background: #404040;
}

QMenuBar::item
{
	spacing: 3px; /* spacing between menu bar items */
	padding: 1px 4px;
	background: transparent;
	color: #c8c8c8;
}

/* when selected using mouse or keyboard */

QMenuBar::item:selected
{
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
	color: #e5e5e5;
	border-radius: 5px;
}

QMenuBar::item:pressed
{
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
	color: #e5e5e5;
	border-radius: 5px;
}

QPushButton
{
	min-width: 80px;
	border-top: 1px solid #818181;
	border-left: 1px solid #818181;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #565656;
	color: #c8c8c8;
	border-radius: 5px;
}

/* all types of push button */
QPushButton:hover
{
	color: #cccccc;
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
	border-radius: 5px;
}

QPushButton:pressed
{
	color: #000000;
	border-top: 1px solid #616161;
	border-left: 1px solid #616161;
	border-bottom: 1px solid #2b2b2b;
	border-right: 1px solid #2b2b2b;
	background: #464646;
	border-radius: 5px;
}

/* Customize radio buttons. */
QRadioButton 
