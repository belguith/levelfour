compositor AmbientOcclusionCompositor
{
    technique
    {
        target_output
        {
			// Start with clear output
            input none

			// Draw a fullscreen quad with the black and white image
            pass render_quad
            {
				identifier 0
				material AmbientOcclusionNode_Shader/AmbientOcclusionMaterial
            }
        }
		
    }
}
