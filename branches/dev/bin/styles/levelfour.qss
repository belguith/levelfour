/* Customize arrows. */

*::down-arrow, *::menu-indicator {
    image: url(:/MainWindow/images/down_arrow.png);
    width: 7px;
    height: 7px;
}

*::down-arrow:disabled, *::down-arrow:off {
   image: url(:/MainWindow/images/down_arrow_disabled.png);
}

*::up-arrow {
    image: url(:/MainWindow/images/up_arrow.png);
    width: 7px;
    height: 7px;
}

*::up-arrow:disabled, *::up-arrow:off {
   image: url(:/MainWindow/images/up_arrow_disabled.png);
}


QScrollBar:horizontal
{
	background: #a0a0a0;
	max-height: 14px;
	min-height: 14px;
	margin: 0px 20px 0 20px;
}

QScrollBar::handle:horizontal
{
	image: url(:/MainWindow/images/hscroll.png);
	min-width: 11px;
	min-height: 11px;
	max-width: 11px;
	max-height: 11px;
}

QScrollBar::add-line:horizontal
{
     background: #aeaeae;
     width: 20px;
     subcontrol-position: right;
     subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal
{
     background: #aeaeae;
     width: 20px;
     subcontrol-position: left;
     subcontrol-origin: margin;
}

QScrollBar:left-arrow:horizontal
{
	image: url(:/MainWindow/images/up_arrow.png);
}

QScrollBar::right-arrow:horizontal
{
	image: url(:/MainWindow/images/up_arrow.png);
}

QScrollBar:vertical 
{
	background: #a0a0a0;
	max-width: 14px;
	min-width: 14px;
	margin: 20px 0 20px 0;
}

QScrollBar::handle:vertical
{
     	image: url(:/MainWindow/images/vscroll.png);
	min-width: 11px;
	min-height: 11px;
	max-width: 11px;
	max-height: 11px;
}

QScrollBar::add-line:vertical
{
     background: #aeaeae;
     height: 20px;
     subcontrol-position: top;
     subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical
{
     background: #aeaeae;
     height: 20px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
}

QScrollBar:up-arrow:vertical
{
	image: url(:/MainWindow/images/up_arrow.png);
}

QScrollBar::down-arrow:vertical
{
	image: url(:/MainWindow/images/down_arrow.png);
}


QSlider::groove:vertical
{
	background: #333333;
	position: absolute; 
	left: 4px; right: 4px;
}

QSlider::handle:vertical
{
     height: 10px;
     background: #777777;
     margin: 0 -4px; /* expand outside the groove */
}

QSlider::add-page:vertical
{
     background: #686868;
}

QSlider::sub-page:vertical
{
     background: #686868;
}

/* Table View */
QTableView
{
	selection-background-color: #565656;
	selection-color: #d2b450;
}

/* TreeView */
QTreeView::item 
{
	background: #bfbfbf;
}

QTreeView::item:hover
{
     	background: #d98c00;
	color: #e5e5e5;
}
QTreeView::item:selected
{
	background: #d98c00;
	color: #e5e5e5;}

QTreeView::item:selected:active
{
 	background: #d98c00;
	color: #e5e5e5;	
}

QTreeView::item:selected:!active
{
     background: #d98c00;
	color: #e5e5e5;}

QTreeView::branch:has-siblings:!adjoins-item {
     border-image: url(:/MainWindow/images/vline.png) 0;
 }

 QTreeView::branch:has-siblings:adjoins-item {
     border-image: url(:/MainWindow/images/branch-more.png) 0;
 }

 QTreeView::branch:!has-children:!has-siblings:adjoins-item {
     border-image: url(:/MainWindow/images/branch-end.png) 0;
 }

 QTreeView::branch:has-children:!has-siblings:closed,
 QTreeView::branch:closed:has-children:has-siblings {
         border-image: none;
         image: url(:/MainWindow/images/branch-closed.png);
 }

 QTreeView::branch:open:has-children:!has-siblings,
 QTreeView::branch:open:has-children:has-siblings  {
         border-image: none;
         image: url(:/MainWindow/images/branch-open.png);
 }

/* The tab widget frame */
QTabWidget::pane
{
	
}

/* Style the tab using the tab sub-control. */
QTabBar::tab
{
	color: #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
	stop: 0 #999999, stop: 1.0 #999999);
	border-top: 1px solid #000000;
	border-left: 1px solid #000000;
	border-right: 1px solid #000000;
	border-bottom: 1px solid #000000; /* #535353 and #4f4f4f */
	padding: 2px;
	margin-left: 0px;
	margin-right: 0px;
	margin-top: 0px;
	margin-bottom: 0px;
	border-radius: 0px;
}

QTabBar::tab:hover
{
	color: #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
	stop: 0 #9b9b9b, stop: 1.0 #8b8b8b);
	border-top: 1px solid #000000;
	border-left: 1px solid #000000;
	border-right: 1px solid #000000;
	border-bottom: 1px solid #000000;
}

QTabBar::tab:selected {
	color: #000000;
	border-top: 1px solid #000000;
	border-left: 1px solid #000000;
	border-bottom: 0px solid #2b2b2b;
	border-right: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
	stop: 0 #bfbfbf, stop: 1.0 #a6a6a6);

}

/* Style for main toolbar */
QToolBar
{
	border-top: 1px solid #cccccc;
	border-bottom: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
	stop: 0 #c9c9c9, stop: 1.0 #a2a2a2 );
	spacing: 3px; /* spacing between items in the tool bar */
	padding-top: 2px;
	padding-bottom: 2px;
	min-height: 21px;
}

/* All types of tool button */
QToolButton
{
	border: 0px solid #cccccc;
	background-color: transparent;
	width: 20px;
	height: 20px;	
}

QToolButton[popupMode="1"]
{
	/* only for MenuButtonPopup */
	padding-right: 20px; /* make way for the popup button */
}

QToolButton::menu-button
{
     /* 16px width + 4px for border = 20px allocated above */
     width: 16px;
 }

QToolButton:hover
{
	border: 0px solid #cccccc;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
	stop: 0 #bababa, stop: 1.0 #bfbfbf);
}

QToolButton:pressed
{
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
	stop: 0 #a1a1a1, stop: 1.0 #b1b1b1);
}

QToolTip
{
	border: 1px solid #555555;
	background: #222222;
	padding: 1px;
	border-radius: 5px;
	opacity: 200;
	color: #CCCCCC;
}
